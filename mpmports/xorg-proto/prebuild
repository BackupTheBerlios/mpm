set -e

cd "$SMPM_BUILDROOT"

for i in `cat "$SMPM_FILES/proto-7.1.list"` ; do
    echo    "*** $i: "
    echo -n "    downloading: "
    urlget ftp://ftp.gwdg.de/pub/x11/x.org/pub/individual/proto/$i > $i
    echo    "ok"
    echo -n "    unpacking: "
    bzip2 -dc "$i" | tar xf -
    echo    "ok"
done

exit 0
